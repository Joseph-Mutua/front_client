roup\">\n        <label>Brand</label>\n        <select\n          type=\"text\"\n          name=\"brand\"\n          className=\"form-control\"\n          onChange={handleChange}\n        >\n          <option value=\"No\">Please Select</option>\n          {brands.map((b) => (\n            <option key={b} value={b}>\n              {b}\n            </option>\n          ))}\n        </select>\n      </div>\n\n      <div className=\"form-group\">\n        <label>Category</label>\n        <select\n          name=\"category\"\n          className=\"form-control\"\n          onChange={handleCategoryChange}\n        >\n          <option>Please select</option>\n          {categories.length > 0 &&\n            categories.map((cat) => (\n              <option key={cat._id} value={cat._id}>\n                {cat.name}\n              </option>\n            ))}\n        </select>\n      </div>\n\n      <div>\n        <label>Sub Categories</label>\n        <Select\n          mode=\"multiple\"\n          style={{ width: \"100%\" }}\n          placeholder=\"Please Select\"\n          value={subcategories}\n          \n          onChange={value => setValues({...values, subcategories: value})}\n        >\n          <option value=\"\">Option one</option>\n          <option value=\"\">Option Two</option>\n        </Select>\n      </div>\n\n      <button className=\"btn btn-outline-info\">Save</button>\n    </form>\n  );\n};\n\nexport default ProductCreateForm;\n",{"ruleId":"161","replacedBy":"162"},{"ruleId":"163","replacedBy":"164"},{"ruleId":"165","severity":1,"message":"166","line":16,"column":6,"nodeType":"167","endLine":16,"endColumn":8,"suggestions":"168"},{"ruleId":"169","severity":1,"message":"170","line":8,"column":3,"nodeType":"171","messageId":"172","endLine":8,"endColumn":19},{"ruleId":"169","severity":1,"message":"173","line":11,"column":10,"nodeType":"171","messageId":"172","endLine":11,"endColumn":14},{"ruleId":"169","severity":1,"message":"174","line":12,"column":10,"nod