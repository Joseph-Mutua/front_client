ge = (e) => {\n    setValues({ ...values, [e.target.name]: e.target.value });\n  };\n\n  const handleCategoryChange = (e) => {\n    e.preventDefault();\n\n    setValues({\n      ...values,\n      subcategories: [],\n      [e.target.name]: e.target.value,\n    });\n    console.log(\"CLICKED CATEGORY\", e.target.value);\n    \n    getSubCategories(e.target.value).then((res) => {\n      console.log(\"SUBCATEGORIES ON CATEGORY CLICK\", res.data);\n      setSubOptions(res.data);\n    });\n    setShowSubs(true);\n  };\n\n  return (\n    <div className=\"container-fluid\">\n      <div className=\"row\">\n        <div className=\"col-md-2\">\n          <AdminNav />\n        </div>\n        \n        <div className=\"col-md-10\">\n          {loading ? (\n            <LoadingOutlined className=\"text-primary\" />\n          ) : (\n            <h4 className=\"text-center\">Create Product</h4>\n          )}\n\n          <hr />\n\n\n          <ProductCreateForm\n            handleSubmit={handleSubmit}\n            handleChange={handleChange}\n            handleCategoryChange={handleCategoryChange}\n            values={values}\n            setValues={setValues}\n            subOptions={subOptions}\n            showSubs={showSubs}\n            setLoading={setLoading}\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default ProductCreate;\n","/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/functions/product.js",[],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/components/forms/ProductCreateForm.js",["175","176","177","178","179"],"import React from \"react\";\nimport { Select } from \"antd\";\n\nimport FileUpload from './FileUpload';\n\nconst { Option } = Select;\n\nconst ProductCreateForm = ({\n  handleSubmit,\n  handleChange,\n  values,\n  setValues,\n  handleCategoryChange,\n  subOptions,\n  showSubs,\n  setLoading,\n}) => {\n  //Destructure values\n  const {\n    title,\n    description,\n    price,\n    categories,\n    category,\n    subcategories,\n    shipping,\n    quantity,\n    images,\n    colors,\n    brands,\n    color,\n\n    brand,\n  } = values;\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <div className=\"form-group\">\n        <label>Title</label>\n        <input\n          type=\"text\"\n          name=\"title\"\n          className=\"form-control\"\n          value={title}\n          onChange={handleChange}\n        />\n      </div>\n\n      <div className=\"form-group\">\n        <label>Description</label>\n        <input\n          type=\"text\"\n          name=\"description\"\n          className=\"form-control\"\n          value={description}\n          onChange={handleChange}\n        />\n      </div>\n\n      <div className=\"form-group\">\n        <label>Price</label>\n        <input\n          type=\"number\"\n          name=\"price\"\n          className=\"form-control\"\n          value={price}\n          onChange={handleChange}\n        />\n      </div>\n\n      <div className=\"form-group\">\n        <label>Shipping</label>\n        <select\n          name=\"shipping\"\n          className=\"form-control\"\n          onChange={handleChange}\n        >\n          <option value=\"No\">Please Select</option>\n          <option value=\"No\">No</option>\n          <option value=\"Yes\">Yes</option>\n        </select>\n      </div>\n\n      <div className=\"form-group\">\n        <label>Quantity</label>\n        <input\n          type=\"number\"\n          name=\"quantity\"\n          className=\"form-control\"\n          value={quantity}\n          onChange={handleChange}\n        />\n      </div>\n\n      <div className=\"form-group\">\n        <label>Color</label>\n        <select\n          type=\"text\"\n          name=\"color\"\n          className=\"form-control\"\n          onChange={handleChange}\n        >\n          <option value=\"No\">Please Select</option>\n          {colors.map((col) => (\n            <option key={col} value={col}>\n              {col}\n            </option>\n          ))}\n        </select>\n      </div>\n\n      <div className=\"form-group\">\n        <label>Brand</label>\n        <select\n          type=\"text\"\n          name=\"brand\"\n          className=\"form-control\"\n          onChange={handleChange}\n        >\n          <option value=\"No\">Please Select</option>\n          {brands.map((b) => (\n            <option key={b} value={b}>\n              {b}\n            </option>\n          ))}\n        </select>\n      </div>\n\n      <div className=\"form-group\">\n        <label>Category</label>\n        <select\n          name=\"category\"\n          className=\"form-control\"\n          onChange={handleCategoryChange}\n        >\n          <option>Please select</option>\n          {categories.length > 0 &&\n            categories.map((cat) => (\n              <option key={cat._id} value={cat._id}>\n                {cat.name}\n              </option>\n            ))}\n        </select>\n      </div>\n\n      {showSubs && (\n        <div>\n          <label>Sub Categories</label>\n          <Select\n            mode=\"multiple\"\n            style={{ width: \"100%\" }}\n            placeholder=\"Please Select\"\n            value={subcategories}\n            onChange={(value) => setValues({ ...values, subcategories: value })}\n          >\n            {subOptions.length &&\n              subOptions.map((s) => (\n                <Option key={s._id} value={s._id}>\n                  {s.name}\n                </Option>\n              ))}\n            )\n          </Select>\n        </div>\n      )}\n\n      <br />\n      <div className=\"p-3\">\n        <FileUpload values={values} setValues={setValues} setLoading={setLoading}/>\n      </div>\n\n      <br />\n      <button className=\"btn btn-outline-info\">Save</button>\n    </form>\n  );\n};\n\nexport default ProductCreateForm;\n","/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/f