 useState(false);\n  const [page, setPage] = useState(1);\n  const [productsCount, setProductsCount] = useState(0);\n\n  useEffect(() => {\n    loadAllProducts();\n  }, [page]);\n\n  useEffect(() => {\n    getProductsCount().then((res) => {\n      setProductsCount(res.data);\n    });\n  }, []);\n\n  const loadAllProducts = () => {\n    setLoading(true);\n    getProducts(\"sold\", \"desc\", page).then((res) => {\n      setProducts(res.data);\n      console.log(products);\n      setLoading(false);\n    });\n  };\n\n  return (\n    <>\n      <div className=\"container\">\n        {loading ? (\n          <LoadingCard count={3} />\n        ) : (\n          <div className=\"row\">\n            {products.map((product) => (\n              <div key={product._id} className=\"col-md-4\">\n                <ProductCard product={product} />\n              </div>\n            ))}\n          </div>\n        )}\n      </div>\n\n      <div className=\"row\">\n        <nav className=\"col-md-4 offset-md-4 text-center pt-5 p-3\">\n          {\" \"}\n          <Pagination\n            current={page}\n            total={(productsCount / 3) * 10}\n            onChange={(value) => setPage(value)}\n          />\n        </nav>\n      </div>\n    </>\n  );\n};\n\nexport default BestSellers;\n","/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/pages/Product.js",["239","240"],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/components/cards/SingleProduct.js",["241","242","243"],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/components/cards/ProductListItems.js",["244"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst ProductListItems = ({ product }) => {\n  const {\n    price,\n    description,\n    category,\n    subcategories,\n    slug,\n    shipping,\n    color,\n    brand,\n    quantity,\n    sold,\n  } = product;\n\n  return (\n    <ul className=\"list-group\">\n      <l