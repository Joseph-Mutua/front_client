n        })\n        .catch((err) => {\n          if (err.response.status === 400) {\n            setLoading(false);\n            toast.error(err.response.data);\n          }\n        });\n    }\n  };\n\n  //Step 4\n  const searched = (keyword) => (c) => c.name.toLowerCase().includes(keyword);\n\n  return (\n    <div className=\"container-fluid\">\n      <div className=\"row\">\n        <div className=\"col-md-2\">\n          <AdminNav />\n        </div>\n        <div className=\"col\">\n          {loading ? (\n            <h4 className=\"text-danger\">Loading...</h4>\n          ) : (\n            <h4 className=\"text-center\">Create Subcategory</h4>\n          )}\n\n          <div className=\"form-group\">\n            <label>Parent Category</label>\n            <select\n              name=\"category\"\n              className=\"form-control\"\n              onChange={(e) => setCategory(e.target.value)}\n            >\n\n              <option>Please select</option>\n              {categories.length > 0 &&\n                categories.map((cat) => (\n                  <option key={cat._id} value={cat._id}>\n                    {cat.name}\n                  </option>\n                ))}\n            </select>\n          </div>\n\n          <CategoryForm\n            handleSubmit={handleSubmit}\n            name={name}\n            setName={setName}\n          />\n\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default SubCategoryCreate;\n","/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/functions/auth.js",[],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/functions/category.js",[],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/functions/subcategory.js",[],"/home/josephm/Desktop/REACT/REACT_TUTORIALS/MERN_ECOMMERCE/front_client/src/pages/admin/subcategory/SubCategoryUpdate..js",["218","219","220","221","222"],"import React, { useState, useEffect } from \"react\";\nim